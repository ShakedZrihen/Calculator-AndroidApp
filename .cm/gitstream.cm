# -*- mode: yaml -*-
manifest:
  version: 1.0

config:
  admins: ['EladKohavi']
automations:
  estimated_time_to_review:
    if:
      - true
    run:
      - action: add-comment@v1
        args:
          comment: |
            {{ pr.description }}
  code_experts:
    if: 
      - {{ pr.title | match(term="shaked") }}
    run:
      - action: add-label@v1
        args:
          label: "/:\\ gitStream"
calc:
  etr: {{ branch | estimatedReviewTime }}
